<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="566.0" prefWidth="276.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tin.tutor.TutorMainWindow">
    <children>
        <Label layoutX="70.0" layoutY="22.0" text="Portal Wykładowcy" textAlignment="CENTER" underline="true">
            <font>
                <Font name="System Bold" size="15.0" />
            </font>
        </Label>
        <Button fx:id="yourConsultationsButton" layoutX="57.0" layoutY="72.0" mnemonicParsing="false" onAction="#btnYourConsultationsClicked" prefHeight="34.0" prefWidth="165.0" text="Dodaj konsultację" />
        <Button fx:id="usersButton" layoutX="57.0" layoutY="231.0" mnemonicParsing="false" onAction="#btnUsersClicked" prefHeight="34.0" prefWidth="165.0" text="Nieaktywni użytkownicy" />
        <Button fx:id="logoutButton" layoutX="55.0" layoutY="384.0" mnemonicParsing="false" onAction="#btnLogoutClicked" prefHeight="34.0" prefWidth="165.0" text="Wyloguj" />
        <TextArea fx:id="console" editable="false" layoutX="17.0" layoutY="440.0" prefHeight="67.0" prefWidth="244.0" />
        <Button fx:id="reconnectButton" layoutX="15.0" layoutY="518.0" mnemonicParsing="false" onAction="#btnreconnectClicked" prefHeight="34.0" prefWidth="244.0" text="Ponów próbę połączenia" />
        <Button fx:id="cancelConsultationButton" layoutX="56.0" layoutY="152.0" mnemonicParsing="false" onAction="#btnCancelConsultationClicked" prefHeight="34.0" prefWidth="165.0" text="Twoje konsultacje" />
      <Button fx:id="requestsButton" layoutX="55.0" layoutY="307.0" mnemonicParsing="false" onAction="#btnRequestsClicked" prefHeight="34.0" prefWidth="165.0" text="Proponowane konsultacje" />
    </children>
</AnchorPane>
