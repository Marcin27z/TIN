<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="434.0" prefWidth="276.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tin.MainPage">
   <children>
      <Label layoutX="30.0" layoutY="30.0" text="System zapisów na konsultacje" textAlignment="CENTER" underline="true">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Button fx:id="yourConsultationsButton" layoutX="70.0" layoutY="78.0" mnemonicParsing="false" onAction="#btnYourConsultationsClicked" prefHeight="34.0" prefWidth="137.0" text="Dodaj konsultację" />
      <Button fx:id="searchButton" layoutX="70.0" layoutY="145.0" mnemonicParsing="false" onAction="#btnSearchClicked" prefHeight="34.0" prefWidth="137.0" text="Wyszukaj konsultację" />
      <Button fx:id="logoutButton" layoutX="70.0" layoutY="222.0" mnemonicParsing="false" onAction="#btnLogoutClicked" prefHeight="34.0" prefWidth="137.0" text="Wyloguj" />
      <TextArea fx:id="console" editable="false" layoutX="16.0" layoutY="287.0" prefHeight="67.0" prefWidth="244.0" />
      <Button fx:id="reconnectButton" layoutX="16.0" layoutY="378.0" mnemonicParsing="false" onAction="#btnreconnectClicked" prefHeight="34.0" prefWidth="244.0" text="Ponów próbę połączenia" />
   </children>
</AnchorPane>
